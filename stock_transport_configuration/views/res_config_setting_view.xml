<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_config_settings_view_form" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.stock.tranport.configuration</field>
            <field name="model">res.config.settings</field>
            <field name="priority" eval="30"/>
            <field name="inherit_id" ref="base.res_config_settings_view_form" />
            <field name="arch" type="xml">
                  <xpath expr="//form/app[@name='stock']/block[@name='shipping_setting_container']" position="inside" >
                        <setting id="module_transport" company_dependent="1" help="Transport management: organize packs in your fleet or carriers">
                              <field name="module_stock_transport"/>
                        </setting>
                  </xpath>
            </field>
        </record>
    </data>
</odoo>